{
  "name": "nmmis",
  "version": "1.0.0",
  "description": "Nepal Municipal Information System",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev:client": "NODE_ENV=development parcel  watch ./frontend/client_src/index.js --out-dir nmmis/static/client/ --no-hmr --no-cache --public-url ./static/client",
    "pro:client": "NODE_ENV=production parcel watch ./frontend/client_src/index.js --out-dir nmmis/static/client/ --no-hmr --no-cache --public-url ./static/client",
    "dev:admin": "NODE_ENV=development parcel  watch ./frontend/admin_src/index.js --out-dir nmmis/static/myadmin/ --no-hmr --no-cache --public-url ./static/myadmin",
    "pro:admin": "NODE_ENV=production parcel watch ./frontend/admin_src/index.js --out-dir nmmis/static/myadmin/ --no-hmr --no-cache --public-url ./static/myadmin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mgrsantox/nmmis.git"
  },
  "author": "Santosh Magar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mgrsantox/nmmis/issues"
  },
  "homepage": "https://github.com/mgrsantox/nmmis#readme",
  "dependencies": {
    "@apollo/client": "^3.1.1",
    "graphql": "^15.3.0",
    "leaflet": "^1.6.0",
    "parcel-bundler": "^1.12.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-google-charts": "^3.0.15",
    "react-leaflet": "^2.7.0",
    "react-leaflet-measure": "^2.1.1",
    "react-router-dom": "^5.2.0",
    "subscriptions-transport-ws": "^0.9.17"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-react": "^7.10.4"
  }
}
